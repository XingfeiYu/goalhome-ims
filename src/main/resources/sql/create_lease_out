CREATE TABLE lease_out (
    id int(4) not null primary key auto_increment,
    lease_start_date DATE,
    lease_end_date DATE,
    start_utility_record int(4),
    end_utility_record int(4),
    lease_unit_id int(4),
    tenant_id int(4),
    agent_id int(4),
    fee_per_month float,
    prepayment float,
    deposit float,
    first_bill_date DATE,
    bill_period int(4),
    pic_url text,
    doc_url text,
    comments text,
    event_status_code TINYINT default 1,
    tstamp TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    deleted TINYINT default 0
)ENGINE=InnoDB DEFAULT CHARSET=utf8;